{
  "name": "tailwindcss-radix-ui-primitives",
  "description": "A plugin for Tailwind CSS v3.2+ that generates data attribute as well as CSS variable utilities for Radix UI.",
  "version": "1.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/junwen-k/tailwindcss-radix-ui-primitives",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint --max-warnings 0",
    "test": "vitest",
    "prepare": "husky",
    "prepublishOnly": "pnpm build"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "tailwindcss": ">=3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.3.0",
    "vitest": "^2.0.5"
  }
}
